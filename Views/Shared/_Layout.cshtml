<!DOCTYPE html>
<meta http-equiv="content-type" content="text/html; charset=utf-8" />

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Document Generation System</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    
    <link href="~/Content/customtheme.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet">

    

    <script src="~/Scripts/jquery-2.1.4.js"> </script>
    <script src="~/Scripts/jquery-2.1.4.min.js"> </script>

    <script src="~/Scripts/bootstrap.js"></script>

    <script src="~/Scripts/jquery.signalR-2.2.0.js"></script>
    <script src="~/Scripts/knockout-3.3.0.js"></script>
    <script src="~/Scripts_ui/ui/jquery.ui.core.js"></script>
    <script src="~/Scripts/jquery.signalR-2.2.0.min.js"></script>
    <script src="~/signalr/hubs"></script>



    <link href="~/Content/tasker.css" rel="stylesheet" />
    

</head>
<body>
    <div id="headerTasker">
        <h1>
            <span class="glyphicon glyphicon-globe"></span>
            Document Generation System
        </h1>
     </div>

    <div id="subHeaderTasker" class="page page-header">
        <div class="cwcSection">
            <div>
                <section>
                    @Html.Partial("_LoginPartial")

                </section>
            </div>

        </div>


    </div>


    <div class="panel-footer">
        @RenderSection("featured", required: false)
        @RenderSection("scripts", required: false)

        <section class="content-wrapper main-content clear-fix">
            @RenderBody()
            <footer class="modal-footer">
                Powered By SRQTID== Syed Rafiuddin Qadri Technology Incubator Designed ,
                Developed. Code:IQRA


            </footer>


        </section>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
   



</body>
</html>
